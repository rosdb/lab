    "@babel/core": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.2.0",
    "file-loader": "4.2.0",
    "html-loader": "0.5.5",
    "live-server": "1.2.1",
    "mini-css-extract-plugin": "0.8.0",
    "netlify-cli": "2.15.0",
    "pug": "2.0.4",
    "pug-cli": "1.0.0-alpha6",
    "pug-html-loader": "1.1.5",
    "webpack": "4.40.2",
    "webpack-cli": "3.3.8",
    "webpack-dev-server": "3.2.1"
  }
